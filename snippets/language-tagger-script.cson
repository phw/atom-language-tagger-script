'.source.tagger_script':
  'Comment':
    'prefix': 'noop'
    'body': '$noop($1)$2'
  '$if(if,then,else)':
    'prefix': 'if'
    'body': '$if(${1:if},${2:then},${3:else})$4'
  '$if2(a1,a2,a3,...)':
    'prefix': 'if2'
    'body': '$if($1)$2'
  '$lower(text)':
    'prefix': 'lower'
    'body': '$lower(${1:text})$2'
  '$upper(text)':
    'prefix': 'upper'
    'body': '$upper(${1:text})$2'
  '$left(text,num)':
    'prefix': 'left'
    'body': '$left(${1:text},${2:num})$3'
  '$right(text,num)':
    'prefix': 'right'
    'body': '$right(${1:text},${2:num})$3'
  '$matchedtracks()':
    'prefix': 'matchedtracks'
    'body': '$matchedtracks()$1'
  '$num(num,len)':
    'prefix': 'num'
    'body': '$num(${1:num},${2:len})$3'
  '$replace(text,search,replace)':
    'prefix': 'replace'
    'body': '$replace(${1:text},${2:search},${3:replace})$4'
  '$rsearch(text,pattern)':
    'prefix': 'rsearch'
    'body': '$rsearch(${1:text},${2:pattern})$3'
  '$rreplace(text,pattern,replace)':
    'prefix': 'rreplace'
    'body': '$rreplace(${1:text},${2:pattern},${3:replace})$4'
  '$in(x,y)':
    'prefix': 'in'
    'body': '$in(${1:x},${2:y})$3'
  '$inmulti(x,y)':
    'prefix': 'inmulti'
    'body': '$inmulti(${1:x},${2:y})$3'
  '$unset(name)':
    'prefix': 'unset'
    'body': '$unset(${1:name})$2'
  '$set(name,value)':
    'prefix': 'set'
    'body': '$set(${1:name},${2:value})$3'
  '$setmulti(name,value,separator="; ")':
    'prefix': 'setmulti'
    'body': '$setmulti(${1:name},${2:value})$3'
  '$get(name)':
    'prefix': 'get'
    'body': '$get(${1:name})$2'
  '$copy(new,old)':
    'prefix': 'copy'
    'body': '$copy(${1:new},${2:old})$3'
  '$copymerge(new,old)':
    'prefix': 'copymerge'
    'body': '$copymerge(${1:new},${2:old})$3'
  '$trim(text[,char])':
    'prefix': 'trim'
    'body': '$trim(${1:text})$2'
  '$add(x,y)':
    'prefix': 'add'
    'body': '$add(${1:x},${2:y})$3'
  '$sub(x,y)':
    'prefix': 'sub'
    'body': '$sub(${1:x},${2:y})$3'
  '$div(x,y)':
    'prefix': 'div'
    'body': '$div(${1:x},${2:y})$3'
  '$mod(x,y)':
    'prefix': 'mod'
    'body': '$mod(${1:x},${2:y})$3'
  '$mul(x,y)':
    'prefix': 'mul'
    'body': '$mul(${1:x},${2:y})$3'
  '$or(x,y)':
