  '$gte(x,y)':
    'prefix': 'gte'
    'body': '$gte(${1:x},${2:y})$3'
  '$len(text)':
    'prefix': 'len'
    'body': '$len(${1:text})$2'
  '$performer(pattern="",join=", ")':
    'prefix': 'performer'
    'body': '$performer()$2'
  '$firstalphachar(text,nonalpha="#")':
    'prefix': 'firstalphachar'
    'body': '$firstalphachar(${1:text})$2'
  '$initials(text)':
    'prefix': 'initials'
    'body': '$initials(${1:text})$2'
  '$truncate(text,length)':
